rem(px)
  return px/14rem
html
    font-size 14px
    background #b8b8b8
body
  overflow: auto
.navbar-inverse
  background #169f85
  border-color white
  color white
.navbar-inverse .navbar-toggle
  border-color white
.navbar-inverse .navbar-toggle:hover
  background none
.navbar-inverse .navbar-brand
  color white
.navbar-inverse .navbar-nav > li > a
  color white
  float left
  padding 5% 8%
.navbar-inverse .navbar-collapse, .navbar-inverse .navbar-form
  border-color white
section#list-warp
  width 100%
  height 100%
  position relative
  top rem(50)
.list-content
  width 100%
  height 100%
  overflow hidden
.list-content ul
  list-style none
  box-sizing border-box
  -webkit-padding-start 0
.list-content ul li
  margin-bottom 2%
  width 100%
  height 100%
  list-style  none
  background white
  box-sizing border-box
.list-content ul li .return-detail
  display block
  width 100%
  height rem(110)
  padding rem(2)
.list-content ul li .return-detail .left-warp
  float left
  width 30%
  height 100%
  box-sizing border-box
.list-content ul li .return-detail .left-warp img
  width 100%
  height 100%
.list-content ul li .return-detail .middle-warp
  float left
  width 50%
  height 100%
  padding 0 rem(5)
  box-sizing border-box
.list-content ul li .return-detail .middle-warp  h3
  overflow hidden
  margin-top 0
  margin-bottom 0
  width 100%
  height 50%
  line-height 98%
  font-size rem(14)
  color #1d1d1d
  word-wrap:break-word
  box-sizing border-box
.list-content ul li .return-detail .middle-warp h3:hover
  color red
  text-decoration underline
.list-content ul li .return-detail .middle-warp p
  width 100%
  height 50%
  line-height rem(52)
  color #9d9d9d
  overflow hidden
  text-overflow: ellipsis
  white-space: nowrap
  box-sizing border-box
.list-content ul li .return-detail .right-warp
  float left
  width 20%
  height 100%
  box-sizing border-box
.list-content ul li .return-detail .right-warp b
  display block
  width 100%
  height 50%
  line-height rem(52)
  text-align center
  box-sizing border-box
.list-content ul li .return-detail .right-warp .join-cart
  display block
  width 100%
  height 50%
  line-height rem(52)
  text-align center
  box-sizing border-box
.list-content ul li .return-detail .right-warp .join-cart img
  width 50%
  height 50%
.friendly-reminder
  display block
  text-align center
footer
  float left
  width 100%
  height rem(100)
  line-height rem(100)
  text-align center
  box-sizing border-box
.footer-hint
  display none
  text-align center
  box-sizing border-box
.foot
  position fixed
  bottom 0
  width 100%
  height rem(50)
  background white
.foot ul
  -webkit-padding-start: 0
.foot ul li
  float left
  width 25%
  height rem(50)
  line-height rem(50)
  text-align center
  list-style none
  background #169f85
  box-sizing border-box
.foot ul li a
  display line-block
  height  65%
  width   80%
  padding  10% 0
  color white
  font-size rem(16px)
  border-bottom 2px solid white
  box-sizing border-box







